@prefix : <http://kik.misis.ru/ontology/mpi/2024/ivanov-ivan/task3/> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

#################################################################
# SWRL Rule: Valid request classification
#################################################################

:Rule_ValidRequest rdf:type swrl:Imp ;

    swrl:body (
        [ swrl:classPredicate :Запрос ;
          swrl:argument1 ?x ]
        [ swrl:propertyPredicate :имеет_автора ;
          swrl:argument1 ?x ;
          swrl:argument2 ?s ]
        [ swrl:classPredicate :Студент ;
          swrl:argument1 ?s ]
        [ swrl:propertyPredicate :обучается_на_программе ;
          swrl:argument1 ?s ;
          swrl:argument2 ?p ]
        [ swrl:classPredicate :ОбразовательнаяПрограмма ;
          swrl:argument1 ?p ]
        [ swrl:propertyPredicate :включает ;
          swrl:argument1 ?p ;
          swrl:argument2 ?r ]
        [ swrl:classPredicate :ЭлектронныйОбразовательныйРесурс ;
          swrl:argument1 ?r ]
        [ swrl:propertyPredicate :обращается_к ;
          swrl:argument1 ?x ;
          swrl:argument2 ?r ]
    ) ;

    swrl:head (
        [ swrl:classPredicate :ВалидныйЗапрос ;
          swrl:argument1 ?x ]
    ) .
