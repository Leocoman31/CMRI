@prefix : <http://example.org/university#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

############################
# Классы
############################

:Person rdf:type owl:Class .
:Student rdf:type owl:Class ;
    rdfs:subClassOf :Person .
:Teacher rdf:type owl:Class ;
    rdfs:subClassOf :Person .
:Course rdf:type owl:Class .

############################
# Объектные свойства
############################

:studies rdf:type owl:ObjectProperty ;
    rdfs:domain :Student ;
    rdfs:range :Course .

:teaches rdf:type owl:ObjectProperty ;
    rdfs:domain :Teacher ;
    rdfs:range :Course .

############################
# Экземпляры (индивиды)
############################

:Alice rdf:type :Student .
:Bob rdf:type :Student .
:DrSmith rdf:type :Teacher .
:DrIvanov rdf:type :Teacher .

:Math101 rdf:type :Course .
:CS102 rdf:type :Course .

############################
# Связи между экземплярами
############################

:Alice :studies :Math101 .
:Bob :studies :CS102 .

:DrSmith :teaches :Math101 .
:DrIvanov :teaches :CS102 .
